    "tabWidth": 4,
    "useTabs": false,
    "singleQuote": true,
    "semi": false,
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "trailingComma": "es5",
    "endOfLine": "crlf",
    "bracketSameLine": true,
    "printWidth": 80,
    "plugins": [
        "prettier-plugin-tailwindcss"
    ]
}